{
  "name": "koffinate/laravel-database",
  "type": "library",
  "description": "Extended laravel database by koffinate",
  "keywords": ["laravel", "framework", "koffinate", "yusron", "yusronarif"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Kfn\\Database\\": ""
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Kfn\\Database\\DatabaseServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
